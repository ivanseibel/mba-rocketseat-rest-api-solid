{
	"name": "mba-rocketseat-rest-api-solid",
	"version": "1.0.0",
	"description": "API REST com SOLID - GymPass style app",
	"main": "index.js",
	"scripts": {
		"dev": "tsx watch src/server.ts",
		"build": "tsup src --out-dir build",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"test:create-prisma-environment": "pnpm link ./prisma/vitest-environment-prisma",
		"test:install-prisma-environment": "pnpm link vitest-environment-prisma",
		"test": "vitest run --dir src/use-cases",
		"test:watch": "vitest --dir src/use-cases",
		"pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
		"test:e2e": "vitest run --dir src/http",
		"test:e2e:watch": "vitest --dir src/http"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "22.13.1",
		"@vitest/coverage-c8": "0.28.5",
		"@vitest/ui": "0.28.5",
		"prisma": "4.10.1",
		"tsup": "8.3.6",
		"tsx": "4.19.2",
		"typescript": "5.7.3",
		"vite-tsconfig-paths": "4.0.5",
		"@types/bcryptjs": "2.4.2",
		"vitest": "0.28.5",
		"npm-run-all": "4.1.5",
		"@types/supertest": "2.0.12",
		"supertest": "6.3.3"
	},
	"dependencies": {
		"@fastify/cookie": "8.3.0",
		"@fastify/jwt": "6.7.0",
		"@prisma/client": "4.10.1",
		"bcryptjs": "2.4.3",
		"dayjs": "1.11.7",
		"dotenv": "16.0.3",
		"fastify": "4.13.0",
		"zod": "3.20.6"
	}
}
